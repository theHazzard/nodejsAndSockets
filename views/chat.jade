extends layout

block content
	h1= title
	h2 Bienvenido #{usuario}
	h3 Listado de Usuarios
	section#chatContainer
		section#chat
		form(action='')
			input(type='submit')#chatInput
		section#users
			if(usuarios)
				each u in usuarios
					p #{u.usuario}			
	form(action='/salir', method='post')
		input(type='submit', value='Salir')